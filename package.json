{
  "name": "form-factor-eas",
  "version": "1.0.0",
  "packageManager": "bun@1.2.22",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:devclient": "expo start --dev-client",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios --configuration Debug",
    "ios:fc": "xcodebuild -workspace ios/formfactoreas.xcworkspace -scheme formfactoreas -configuration Debug -sdk iphoneos26.1 -destination 'name=fc' build",
    "ios:device": "xcodebuild -workspace ios/formfactoreas.xcworkspace -scheme formfactoreas -configuration Debug -sdk iphoneos26.1 -destination 'name=fc' build",
    "web": "expo start --web",
    "lint": "expo lint",
    "kill": "lsof -ti :8081 | xargs kill -9 && lsof -ti :8082 | xargs kill -9 && lsof -ti :8083 | xargs kill -9 && lsof -ti :8084 | xargs kill -9 && lsof -ti :8085 | xargs kill -9",
    "fix:ios": "./scripts/fix-ios-build.sh",
    "screenshot": "./scripts/capture-device-screen.sh",
    "watch:device": "./scripts/watch-device-screen.sh",
    "docs:arkit": "cat docs/ARKIT_BODY_TRACKING_GUIDE.md",
    "docs:build": "cat docs/IOS_BUILD_AND_DEBUG_GUIDE.md",
    "build:local:ios": "./scripts/build-local-ios.sh",
    "build:local:android:aab": "./scripts/build-local-android.sh aab",
    "build:local:android:apk": "./scripts/build-local-android.sh apk",
    "test:swift": "open ios/formfactoreas.xcworkspace",
    "logs:ios": "xcrun simctl spawn booted log stream --predicate 'processImagePath endswith \"formfactoreas\"' --level=debug",
    "check:dead-code": "knip --include files,exports,dependencies",
    "check:dead-code:all": "knip"
  },
  "resolutions": {
    "framer-motion": "^12.18.1",
    "expo-file-system": "19.0.16",
    "expo-web-browser": "15.0.8",
    "@expo/fingerprint": "0.15.1"
  },
  "dependencies": {
    "@expo-google-fonts/lexend": "^0.2.3",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/native": "^7.1.6",
    "@supabase/supabase-js": "^2.50.0",
    "arkit-body-tracker": "file:./modules/arkit-body-tracker",
    "buffer": "^6.0.3",
    "expo": "54.0.12",
    "expo-apple-authentication": "~8.0.7",
    "expo-auth-session": "~7.0.8",
    "expo-constants": "~18.0.9",
    "expo-crypto": "~15.0.7",
    "expo-font": "~14.0.8",
    "expo-haptics": "^15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-network": "^8.0.7",
    "expo-router": "~6.0.10",
    "expo-sqlite": "^16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "moti": "^0.30.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "^0.81.4",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-health": "^1.19.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.14.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vision-camera": "^4.7.2",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "react-native-worklets-core": "^1.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@playwright/test": "^1.53.1",
    "@types/react": "^19.1.14",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "expo-module-scripts": "^4.1.7",
    "knip": "^5.66.2"
  },
  "expo": {
    "autolinking": {
      "modules": [
        "./modules/arkit-body-tracker"
      ]
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": ["react-native-health", "react-native-watch-connectivity"]
      }
    }
  },
  "private": true
}

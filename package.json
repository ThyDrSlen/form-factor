{
  "name": "form-factor-eas",
  "version": "1.0.0",
  "packageManager": "bun@1.2.22",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:devclient": "APP_VARIANT=development expo start --dev-client",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "APP_VARIANT=development expo run:android",
    "ios": "APP_VARIANT=development expo run:ios --configuration Debug",
    "ios:prod": "expo run:ios --configuration Debug",
    "ios:fc": "./scripts/ios-device-build.sh fc",
    "ios:device": "xcodebuild -workspace ios/formfactoreas.xcworkspace -scheme formfactoreas -configuration Debug -sdk iphoneos26.1 -destination 'name=fc' build",
    "ios:timed": "./scripts/measure-ios-run.sh",
    "ios:timed:fc": "./scripts/measure-ios-run.sh fc",
    "web": "expo start --web",
    "lint": "eslint --config ./etc/eslint.config.mjs app components",
    "check:types": "tsc --noEmit",
    "ci:push": "python3 scripts/ci_local.py --quick",
    "ci:full": "python3 scripts/ci_local.py",
    "ci:local": "bun run lint && bun run check:types",
    "kill": "lsof -ti :8081 | xargs kill -9 && lsof -ti :8082 | xargs kill -9 && lsof -ti :8083 | xargs kill -9 && lsof -ti :8084 | xargs kill -9 && lsof -ti :8085 | xargs kill -9",
    "fix:ios": "./scripts/fix-ios-build.sh",
    "screenshot": "./scripts/capture-device-screen.sh",
    "watch:device": "./scripts/watch-device-screen.sh",
    "watch:install": "./scripts/install-watch-sim.sh",
    "docs:arkit": "cat docs/ARKIT_BODY_TRACKING_GUIDE.md",
    "build:local:ios": "./scripts/build-local-ios.sh",
    "build:local:android:aab": "./scripts/build-local-android.sh aab",
    "build:local:android:apk": "./scripts/build-local-android.sh apk",
    "preview:local:submit": "bash scripts/eas-local-preview-submit.sh",
    "test:swift": "open ios/formfactoreas.xcworkspace",
    "logs:ios": "xcrun simctl spawn booted log stream --predicate 'processImagePath endswith \"formfactoreas\"' --level=debug",
    "check:dead-code": "knip --config ./config/knip.json --include files,exports,dependencies",
    "check:dead-code:all": "knip --config ./config/knip.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:report": "jest 2>&1 | tee test-results.txt",
    "test:e2e": "playwright test",
    "prepare": "husky",
    "postinstall": "patch-package"
  },
  "resolutions": {
    "framer-motion": "^12.18.1",
    "expo-file-system": "19.0.16",
    "expo-web-browser": "15.0.8",
    "@expo/fingerprint": "0.15.1",
    "node-forge": "^1.3.2",
    "react-server-dom-webpack": "19.2.3",
    "hono": "4.11.4",
    "qs": "6.14.1"
  },
  "dependencies": {
    "@expo-google-fonts/lexend": "^0.4.1",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.5.1",
    "@supabase/supabase-js": "^2.90.1",
    "arkit-body-tracker": "file:modules/arkit-body-tracker",
    "buffer": "^6.0.3",
    "expo": "54.0.30",
    "expo-apple-authentication": "~8.0.8",
    "expo-application": "~7.0.8",
    "expo-auth-session": "~7.0.10",
    "expo-av": "~16.0.8",
    "expo-constants": "~18.0.13",
    "expo-crypto": "~15.0.8",
    "expo-device": "~8.0.10",
    "expo-document-picker": "^14.0.8",
    "expo-font": "~14.0.10",
    "expo-haptics": "^15.0.8",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-media-library": "~18.2.1",
    "expo-network": "^8.0.8",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.21",
    "expo-speech": "^14.0.8",
    "expo-sqlite": "^16.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-video": "^3.0.15",
    "expo-video-thumbnails": "~10.0.8",
    "expo-web-browser": "~15.0.8",
    "moti": "^0.30.0",
    "nativewind": "^4.2.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-native": "^0.83.1",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.2.1",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.19.0",
    "react-native-svg": "^15.15.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.2",
    "react-native-webview": "13.16.0",
    "react-native-worklets": "^0.7.1",
    "ff-watch-connectivity": "file:modules/ff-watch-connectivity",
    "ff-healthkit": "file:modules/ff-healthkit",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "^19.2.7",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.39.2",
    "eslint-config-expo": "~10.0.0",
    "expo-mcp": "~0.2.3",
    "expo-module-scripts": "^5.0.8",
    "husky": "^9.1.7",
    "jest-expo": "^54.0.16",
    "knip": "^5.80.2",
    "patch-package": "^8.0.1",
    "react-test-renderer": "19.2.3",
    "hono": "4.11.4"
  },
  "private": true,
  "trustedDependencies": [
    "unrs-resolver"
  ]
}

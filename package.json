{
  "name": "form-factor-eas",
  "version": "1.0.0",
  "packageManager": "bun@1.2.22",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:devclient": "APP_VARIANT=development expo start --dev-client",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "APP_VARIANT=development expo run:android",
    "ios": "APP_VARIANT=development expo run:ios --configuration Debug",
    "ios:prod": "expo run:ios --configuration Debug",
    "ios:fc": "./scripts/ios-device-build.sh fc",
    "ios:device": "xcodebuild -workspace ios/formfactoreas.xcworkspace -scheme formfactoreas -configuration Debug -sdk iphoneos26.1 -destination 'name=fc' build",
    "ios:timed": "./scripts/measure-ios-run.sh",
    "ios:timed:fc": "./scripts/measure-ios-run.sh fc",
    "web": "expo start --web",
    "lint": "eslint --config ./etc/eslint.config.mjs app components",
    "check:types": "tsc --noEmit",
    "ci:local": "bun run lint && bun run check:types",
    "kill": "lsof -ti :8081 | xargs kill -9 && lsof -ti :8082 | xargs kill -9 && lsof -ti :8083 | xargs kill -9 && lsof -ti :8084 | xargs kill -9 && lsof -ti :8085 | xargs kill -9",
    "fix:ios": "./scripts/fix-ios-build.sh",
    "screenshot": "./scripts/capture-device-screen.sh",
    "watch:device": "./scripts/watch-device-screen.sh",
    "docs:arkit": "cat docs/ARKIT_BODY_TRACKING_GUIDE.md",
    "build:local:ios": "./scripts/build-local-ios.sh",
    "build:local:android:aab": "./scripts/build-local-android.sh aab",
    "build:local:android:apk": "./scripts/build-local-android.sh apk",
    "test:swift": "open ios/formfactoreas.xcworkspace",
    "logs:ios": "xcrun simctl spawn booted log stream --predicate 'processImagePath endswith \"formfactoreas\"' --level=debug",
    "check:dead-code": "knip --config ./config/knip.json --include files,exports,dependencies",
    "check:dead-code:all": "knip --config ./config/knip.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:report": "jest 2>&1 | tee test-results.txt",
    "test:e2e": "playwright test",
    "prepare": "husky"
  },
  "resolutions": {
    "framer-motion": "^12.18.1",
    "expo-file-system": "19.0.16",
    "expo-web-browser": "15.0.8",
    "@expo/fingerprint": "0.15.1",
    "node-forge": "^1.3.2"
  },
  "dependencies": {
    "@expo-google-fonts/lexend": "^0.2.3",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@supabase/supabase-js": "^2.86.0",
    "buffer": "^6.0.3",
    "expo": "54.0.12",
    "expo-apple-authentication": "~8.0.7",
    "expo-application": "~7.0.7",
    "expo-auth-session": "~7.0.9",
    "expo-av": "~16.0.7",
    "expo-constants": "~18.0.10",
    "expo-crypto": "~15.0.7",
    "expo-device": "~8.0.9",
    "expo-document-picker": "^14.0.7",
    "expo-font": "~14.0.9",
    "expo-haptics": "^15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.9",
    "expo-network": "^8.0.7",
    "expo-notifications": "~0.30.7",
    "expo-router": "~6.0.15",
    "expo-speech": "^14.0.7",
    "expo-sqlite": "^16.0.9",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "expo-video": "^3.0.14",
    "expo-video-thumbnails": "~10.0.7",
    "expo-web-browser": "~15.0.8",
    "arkit-body-tracker": "file:modules/arkit-body-tracker",
    "moti": "^0.30.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "^0.81.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-health": "^1.19.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.5",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.15.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vision-camera": "^4.7.3",
    "react-native-watch-connectivity": "^1.1.0",
    "react-native-web": "^0.21.2",
    "react-native-webview": "13.15.0",
    "react-native-worklets-core": "^1.6.2",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "^19.2.7",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.39.1",
    "eslint-config-expo": "~10.0.0",
    "expo-module-scripts": "^4.1.10",
    "husky": "^9.1.7",
    "jest-expo": "^54.0.13",
    "knip": "^5.70.2"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-health",
          "react-native-watch-connectivity"
        ]
      }
    }
  },
  "private": true,
  "trustedDependencies": [
    "unrs-resolver"
  ]
}

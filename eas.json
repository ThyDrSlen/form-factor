{
  "cli": {
    "version": ">=5.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "buildConfiguration": "Debug",
        "image": "macos-sequoia-15.5-xcode-26.0"
      },
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "cache": {
        "disabled": true
      },
      "ios": {
        "simulator": false,
        "buildConfiguration": "Debug",
        "image": "macos-sequoia-15.5-xcode-26.0",
        "credentialsSource": "remote"
      },
      "android": { "buildType": "apk" },
      "env": {
        "APP_VARIANT": "preview"
      }
    },
    "staging": {
      "distribution": "store",
      "ios": {
        "buildConfiguration": "Release",
        "image": "macos-sequoia-15.5-xcode-26.0"
      },
      "env": {
        "APP_VARIANT": "staging",
        "EXPO_USE_NEW_ARCH": "1"
      },
      "android": { "buildType": "app-bundle" }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "ios": {
        "buildConfiguration": "Release",
        "image": "macos-sequoia-15.5-xcode-26.0"
      },
      "env": {
        "APP_VARIANT": "production",
        "EXPO_USE_NEW_ARCH": "1"
      },
      "android": { "buildType": "app-bundle" }
    }
  },
  "submit": {
    "staging": {
      "android": { "track": "internal", "releaseStatus": "draft" },
      "ios": { "appleTeamId": "NCTLNFGC6G", "ascAppId": "675470330" }
    },
    "production": {
      "android": { "track": "production", "releaseStatus": "draft" },
      "ios": { "appleTeamId": "NCTLNFGC6G", "ascAppId": "675470330" }
    }
  }
}
